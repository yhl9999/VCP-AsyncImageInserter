# AsyncImageInserter 插件配置文件示例
# 复制此文件为 config.env 并填入实际配置值

# 基础配置
DEBUG_MODE=true
IMAGE_GENERATOR_AGENT_ID=ImageGenerator

# 任务管理
MAX_CONCURRENT_TASKS=3
TASK_TIMEOUT=300000
DEFAULT_PRIORITY=normal

# 占位符配置  
PLACEHOLDER_PREFIX=ASYNC_IMG

# 优先级权重
HIGH_PRIORITY_WEIGHT=10
NORMAL_PRIORITY_WEIGHT=5
LOW_PRIORITY_WEIGHT=1

# 清理配置
PLACEHOLDER_CLEANUP_INTERVAL=600000
PLACEHOLDER_MAX_AGE=3600000